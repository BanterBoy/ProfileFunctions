{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#61AFEF",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "folder"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
},
"console_title_template": "{{ .Shell }} in {{ .Folder }}",
"final_space": true,
"palette": {
  "black": "#262B44",
  "blue": "#4B95E9",
  "green": "#59C9A5",
  "orange": "#F07623",
  "red": "#D81E5B",
  "white": "#E0DEF4",
  "yellow": "#F3AE35"
}